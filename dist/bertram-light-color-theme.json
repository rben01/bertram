	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment",
				"string.docstring",
				"punctuation.definition.string julia"
			],
			"settings": {
				"foreground": "#8893a0"
			}
		},
		{
			"scope": [
				"variable.other.constant",
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#005cc5"
			}
		},
		{
			"scope": [
				"entity.name",
				"storage.type.haskell",
				"meta.export.default",
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#e38509"
			}
		},
		{
			"scope": [
				"variable.parameter.function",
				"meta.jsx.children",
				"meta.block",
				"meta.tag.attributes",
				"entity.name.constant",
				"meta.object.member",
				"meta.embedded.expression",
				"keyword.operator.update",
				"keyword.operator.assignment",
				"meta.function.call punctuation.brackets"
			],
			"settings": {
				"foreground": "#24292e"
			}
		},
